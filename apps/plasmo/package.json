{
  "name": "plasmo",
  "displayName": "Plasmo",
  "version": "0.0.1",
  "description": "A basic Plasmo extension that authenticates with NextAuth",
  "author": "p6l-richard",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.0.4",
    "@tanstack/react-query": "^4.23.0",
    "plasmo": "0.64.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.20.2",
    "@acme/db": "*"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "key": "$PLASMO_PUBLIC_MANIFEST_KEY",
    "externally_connectable": {
      "matches": [
        "*://localhost/*"
      ]
    }
  }
}
